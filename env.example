# Firebase 설정 (사용자가 직접 입력해야 함)
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id_here
FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
FIREBASE_APP_ID=your_app_id_here

# 한국투자증권 API 설정 (사용자가 직접 입력해야 함)
KIS_API_KEY=your_kis_api_key_here
KIS_SECRET_KEY=your_kis_secret_key_here
KIS_APP_KEY=your_kis_app_key_here
KIS_APP_SECRET=your_kis_app_secret_here

# 개발 환경 설정
NODE_ENV=development
PORT=3000

# 백엔드 서버 설정
BACKEND_PORT=8000
BACKEND_HOST=localhost

# 암호화 키 (사용자가 설정해야 함)
ENCRYPTION_KEY=your_encryption_key_here

# Vercel 환경 변수 (배포 시 사용)
VERCEL_URL=
VERCEL_ENV=

# 로그 레벨
LOG_LEVEL=info

# 테스트 환경
TEST_DATABASE_URL=
TEST_API_ENDPOINT=

# 보안 설정
CORS_ORIGIN=http://localhost:3000
JWT_SECRET=your_jwt_secret_here

# 알림 설정
NOTIFICATION_ENABLED=true
EMAIL_SERVICE_API_KEY=your_email_service_key_here

# 백테스팅 설정
MAX_BACKTEST_DURATION=30
BACKTEST_DATA_RETENTION_DAYS=90

# 자동매매 설정
AUTO_TRADING_ENABLED=false
MAX_POSITION_SIZE=1000000
RISK_MANAGEMENT_ENABLED=true 